#Directory structure of configuration files

#~/
~/.Xresources

#i3
~/.config/i3/config
~/.config/i3/nord-wallpaper.png

#i3status
~/.config/i3status/config

#rofi
~/.config/rofi/config
~/.config/rofi/nord.rasi
~/.config/rofi/rofi-power-menu

#dunst
~/.config/dunst/dunstrc

#picom
~/.config/picom/picom.conf
